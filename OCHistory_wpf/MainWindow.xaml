        <WebBrowser Margin="12,122,12,0" Name="conversationWebBrowser" UseLayoutRounding="False" ClipToBounds="False" DataContext="{Binding}" />
        <TreeView Height="116" HorizontalAlignment="Stretch" Name="sessionTreeView" VerticalAlignment="Top" Margin="12,0" HorizontalContentAlignment="Left" VerticalContentAlignment="Top"></TreeView>
    </Grid>
</Window>
